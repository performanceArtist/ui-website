{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["eslint-plugin-prettier"],
    "env": {
        "browser": true,
        "jquery": true
    },
    "rules": {
        "linebreak-style":"off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "class-methods-use-this": "off",
        "no-new": "off",
        "no-underscore-dangle": "off"
    }
}